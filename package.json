{
  "name": "ionic-custom-components",
  "version": "1.0.1-0",
  "description": "Custom components for ionic vue projects",
  "main": "./dist/IonicCustomComponents.umd.cjs",
  "module": "./dist/IonicCustomComponents.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/IonicCustomComponents.js",
      "require": "./dist/IonicCustomComponents.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "serve": "vite preview",
    "patch" : "vite build && npm version patch && npm publish",
    "minior" : "vite build && npm version patch && npm minior",
    "major" : "vite build && npm version patch && npm major"
  },
  "keywords": [
    "vue",
    "vue3",
    "component",
    "library",
    "ionic",
    "search",
    "pagination"
  ],
  "author": "joc-luis",
  "peerDependencies": {
    "@ionic/vue": "^7.0.0",
    "vue": "^3.4.19"
  },
  "devDependencies": {
    "@capacitor/device": "^5.0.7",
    "@capacitor/filesystem": "5.2.1",
    "@capawesome/capacitor-file-picker": "^5.3.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vue": "^3.4.19",
    "vue-router": "4.3.0",
    "vue-tsc": "^1.8.27"
  },
  "license": "MIT",
  "repository": "https://github.com/joc-luis/ionic-custom-components"
}
